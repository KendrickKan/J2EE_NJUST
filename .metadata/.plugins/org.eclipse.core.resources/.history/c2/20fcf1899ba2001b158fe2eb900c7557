package njust.dataclass;

public class LoginService {
	public int finishLogin(Login login){
		LoginDAO logdao = new LoginDAO();
		Login testLogin = logdao.getLogin(login.getUserid());
		if(testLogin == null)
			return 201;//没有这个id
		if(testLogin.getPassword() == login.getPassword())
		{
			return 200;//成功
		}
		else return 202;//密码错误
	}
}
