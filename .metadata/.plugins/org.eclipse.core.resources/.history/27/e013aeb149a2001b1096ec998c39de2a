package njust.dataclass;

public class Login {
	private String userid;
	private String name;
	private String password;
	public Login(){
		
	}
	public Login(String id){
		userid = id;
		name = "";
		password = "";
	}
	public void setUserid(String id){
		this.userid = id;
	}
	public String getUserid(){
		return this.userid;
	}
}
